<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.physicscalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OneDActivity"
            android:label="One Dimensional Motion" />
        <activity
            android:name=".TwoDActivity"
            android:label="Two Dimensional Motion" />
        <activity
            android:windowSoftInputMode="adjustPan"
            android:name=".ForcesActivity"
            android:label="Forces" />
        <activity android:name=".CalcReady"/>
    </application>

</manifest>